@model IEnumerable<Link2Web.Models.SettingType>

@{
    ViewBag.Title = "Index";
}

<div id="main">

    <h2>Index</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Type)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.SettingTypeId }) |
                    @Html.ActionLink("Details", "Details", new { id = item.SettingTypeId }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.SettingTypeId })
                </td>
            </tr>
        }

    </table>
    </div>
